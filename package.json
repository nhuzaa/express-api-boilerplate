    "knex": "^0.14.2",
    "pg": "^7.4.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "eslint": "^4.16.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.5.0",
    "nodemon": "^1.14.11",
    "prettier": "^1.10.2"
  },
  "engines": {
    "node": ">= 6.9.0",
    "npm": ">= 3.10.8"
  }
